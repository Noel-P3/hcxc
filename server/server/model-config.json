{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "firebird",
    "public": true,
    "options": {
      "remoting": {
        "shareMethods": {
          "*": false,
          "getUser": true,
          "modifyUser": true
        }
      }
    }
  },
  "Client": {
    "dataSource": "firebird",
    "public": true,
    "options": {
      "remoting": {
        "shareMethods": {
          "*": false,
          "get": true,
          "modifyUser": true
        }
      }
    }
  },
  "Seller": {
    "dataSource": "firebird",
    "public": true,
    "options": {
      "remoting": {
        "shareMethods": {
          "*": false,
          "get": true,
          "modifyUser": true
        }
      }
    }
  }
}
